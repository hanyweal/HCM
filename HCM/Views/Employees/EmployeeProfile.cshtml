    function ResetPrintAll(dataTable) {
        var tblID = "#" + dataTable.attr('id');
        var RowsCount = GetRowsCount(tblID);
        if (RowsCount > 0) {
            $(tblID + " tfoot #aPrintAll").show();
        }
        else
            $(tblID + " tfoot #aPrintAll").hide();
    }

