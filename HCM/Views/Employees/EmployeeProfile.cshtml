    function ResetDataTables() {
        var tables = $('.dataTable').DataTable();
        for (var i = 0; i < tables.context.length; i++) {
            tables.table(i).clear().draw();
            $("#" + tables.table(i).node().id + " tfoot #aPrintAll").hide();
        }
    }

