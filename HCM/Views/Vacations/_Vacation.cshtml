    function GetBalanceTableByVacationType() {
        var EmployeeCodeID = $('#hdnFldEmployeeCodeID').val() || '@Model.EmployeeCodeID';
        var VacationTypeID = $('#ddlVacationTypes').val() || '@Model.VacationType.VacationTypeID';
        var tableName = "tblSickLeaveBalanceTable";

        if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Exceptional)') {
            tableName = "tblExceptionalLeaveBalanceTable";
        }
        else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Sick)') {
            tableName = "tblSickLeaveBalanceTable";
        }
        else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.AccompanimentSick)') {
            tableName = "tblAccompanimentLeaveBalanceTable";
        }
        else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Accompaniment)') {
            tableName = "tblAccompanimentBalanceTable";
        }

        $.ajax({
            type: 'POST',
            dataType: 'json',
            contentType: "application/json; charset=utf-8",
            url: "@Url.Content("~/Vacations/GetBalanceTableByVacationType/")" + EmployeeCodeID + "/" + VacationTypeID,
            success: function (result) {
                $("#" + tableName).DataTable({
                    "destroy": true,
                    "searching": false,
                    "ordering": false,
                    "paging": false,
                    "data": result.data,
                    "columns": [
                        {
                            "data": "StartDate",
                            "autowidth": false,
                            "render": RenderDate
                        },
