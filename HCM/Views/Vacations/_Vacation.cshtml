        $('#txtVacationStartDate').keydown(function (e) {
            var code = e.keyCode || e.which;
            if (code === 9) {
                e.preventDefault();
                GetVacationEndDate("#txtVacationStartDate", "#txtVacationPeriod", "#txtVacationEndDate", "#txtWorkDate");
                $("#txtVacationPeriod").focus();

                @*var VacationTypeID = $('#ddlVacationTypes').val() || '@Model.VacationType.VacationTypeID';
                var EmployeeCodeID = $('#hdnFldEmployeeCodeID').val() || '@Model.EmployeeCodeID';
                var VacationStartDate = $('#txtVacationStartDate').val();
