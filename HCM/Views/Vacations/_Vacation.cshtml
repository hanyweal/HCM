        var VacationStartDate = $('#txtVacationStartDate').val();
        GetVacationBalance('@Model.EmployeeCodeID', '@Model.VacationType.VacationTypeID', VacationStartDate);

        $("#ddlHolidayType").on('change', function (event) {
            var MaturityYearID = $("#ddlMaturityYear").val();
            var HolidayTypeID = $("#ddlHolidayType").val();
            VacationTypeID = $('#ddlVacationTypes').val() || '@Model.VacationType.VacationTypeID';
            EmployeeCodeID = $('#hdnFldEmployeeCodeID').val() || '@Model.EmployeeCodeID';
            if (HolidayTypeID != '') {
                if (MaturityYearID != '') {
                    $.ajax({
                        type: "POST"
                        , url: "@Url.Content("~/Vacations/GetCompensatoryVacationBalance/")" + EmployeeCodeID + "/" + VacationTypeID + "/" + MaturityYearID + "/" + HolidayTypeID
                        , success: function (result) {
                            $("#txtCompensatoryVacationBalance").val(result.data.VacationBalance);
                            $("#txtCompensatoryVacationConsumedBalance").val(result.data.VacationConsumedBalance);
                            $("#txtCompensatoryVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                            $("#txtCompensatoryVacationExpiryDateText").val(RenderDate(result.data.VacationExpiredDate) == '1355-12-NaN' ? '' : RenderDate(result.data.VacationExpiredDate));
                        }
                    , error: function (xhr, status, error) {
                        ResetCompensatoryVacationType();
                    }
                    });
                }
                else {
                    ResetCompensatoryVacationType();
                }
            }
            else {
                ResetCompensatoryVacationType();
            }
        });

        $("#ddlMaturityYear").on('change', function (event) {
            ResetCompensatoryVacationType();
        });

        $("#ddlSportsSeasonMaturityYearID").on('change', function (event) {
            ResetSportsVacationType();

            var SportsSeasonMaturityYearID = $("#ddlSportsSeasonMaturityYearID").val();
            if (SportsSeasonMaturityYearID != '') {
                $.ajax({
                    type: "POST"
                    , url: "@Url.Content("~/Vacations/GetSportsSeasonDescription/")" + SportsSeasonMaturityYearID
                    , success: function (result) {
                        $("#ddlSportsSeasonDescription").html("");
                        var SportsSeasonDescriptionTypes = result.data;
                        $.each(SportsSeasonDescriptionTypes, function (i, data) {
                            $("#ddlSportsSeasonDescription").append($('<option></option>').val(data.SportsSeasonID).html(data.SportsSeasonDescription));
                        });
                        insertSelectionOptionToDDL('ddlSportsSeasonDescription');
                    }
                , error: function (xhr, status, error) {
                    ResetSportsVacationType();
                }
                });
            }
            else {
                ResetSportsVacationType();
            }
        });

        $("#ddlSportsSeasonDescription").on('change', function (event) {
            var SportsSeasonID = $("#ddlSportsSeasonDescription").val();
            VacationTypeID = $('#ddlVacationTypes').val() || '@Model.VacationType.VacationTypeID';
            EmployeeCodeID = $('#hdnFldEmployeeCodeID').val() || '@Model.EmployeeCodeID';
            if (SportsSeasonID != '' && SportsSeasonID != '0') {
                $.ajax({
                    type: "POST"
                    , url: "@Url.Content("~/Vacations/GetBySportsSeasonID/")" + SportsSeasonID
                    , success: function (result) {
                        $("#txtSportsSeasonStartDate").val(RenderDate(result.data.SportsSeasonStartDate));
                        $("#txtSportsSeasonEndDate").val(RenderDate(result.data.SportsSeasonEndDate));
                        @*console.log('url', "@Url.Content("~/Vacations/GetSportsVacationBalance/")" + EmployeeCodeID + "/" + VacationTypeID + "/" + SportsSeasonID);*@
                        $.ajax({
                            type: "POST"
                            , url: "@Url.Content("~/Vacations/GetSportsVacationBalance/")" + EmployeeCodeID + "/" + VacationTypeID + "/" + SportsSeasonID
                            , success: function (result) {

                                // console.log(result);
                                $("#txtSportsVacationBalance").val(result.data.VacationBalance);
                                $("#txtSportsVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                                $("#txtSportsVacationConsumedBalance").val(result.data.VacationConsumedBalance);
                            }
                        , error: function (xhr, status, error) {
                            ResetSportsVacationType();
                        }
                        });
                    }
                , error: function (xhr, status, error) {
                    ResetSportsVacationType();
                }
                });
            }
            else {
                ResetSportsVacationType();
            }
        });

    });

    function GetVacationBalance(EmployeeCodeID, VacationTypeID, VacationStartDate) {

        // some basic validation to call service with wrong parameter or empty parameters.
        if (!EmployeeCodeID) {
            ResetVacationBalance();
            return;
        }

        if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Normal)') {
            VacationStartDate = '@DateTime.Now.ToString(@System.Configuration.ConfigurationManager.AppSettings["DateFormat"])'  //formatDate(new Date()); //new Date();
        }

        var url = "@Url.Content("~/Vacations/GetVacationBalance/")" + EmployeeCodeID + "/" + VacationTypeID;

        //no need to pass userinput date in getBalance, now it based on current date, we are passing dummy date
        VacationStartDate = '1441-01-01';
        url = url + "/" + VacationStartDate;

        /*
        if (VacationStartDate)
            url = url + "/" + VacationStartDate;
        else
            url = url + "/01-01-1318";
        */

        if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Compensatory)') {
            var MaturityYearID = $("#ddlMaturityYear").val();
            var HolidayTypeID = $("#ddlHolidayType").val();
            url = "@Url.Content("~/Vacations/GetCompensatoryVacationBalance/")" + EmployeeCodeID + "/" + VacationTypeID + "/" + MaturityYearID + "/" + HolidayTypeID;
        }
        if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Sports)') {
            var SportsSeasonID;
            if ('@Model.SportsSeasonID' == 0) {
                // console.log('Model', '@Model.SportsSeasonID');
                SportsSeasonID = $("#ddlSportsSeasonDescription").val();
            }
            else {
                //console.log('Model', '@Model.SportsSeasonID');
                $("#ddlSportsSeasonDescription").val('@Model.SportsSeasonID');

                SportsSeasonID = '@Model.SportsSeasonID';
            }
            url = "@Url.Content("~/Vacations/GetSportsVacationBalance/")" + EmployeeCodeID + "/" + VacationTypeID + "/" + SportsSeasonID;
        }
        //console.log('top', url);
        $.ajax({
            type: 'POST',
            dataType: 'json',
            contentType: "application/json; charset=utf-8",
            url: url,
            success: function (result) {
                if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Exceptional)') {
                    //$("#txtExceptionalVacationBalance").val(result.data.VacationBalance);
                    //$("#txtExceptionalVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                    //$("#txtExceptionalVacationConsumedBalance").val(result.data.VacationConsumedBalance);

                    GetBalanceTableByVacationType();
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Sick)') {
                    //$("#txtSickVacationBalance").val(result.data.VacationBalance);
                    //$("#txtSickVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                    //$("#txtSickVacationConsumedBalance").val(result.data.VacationConsumedBalance);

                    GetBalanceTableByVacationType();
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.AccompanimentSick)') {
                    //$("#txtAccompanimentVacationBalance").val(result.data.VacationBalance);
                    //$("#txtAccompanimentVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                    //$("#txtAccompanimentVacationConsumedBalance").val(result.data.VacationConsumedBalance);

                    GetBalanceTableByVacationType();
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Accompaniment)') {
                    //$("#txtAccompanimentVacationBalance").val(result.data.VacationBalance);
                    //$("#txtAccompanimentVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                    //$("#txtAccompanimentVacationConsumedBalance").val(result.data.VacationConsumedBalance);

                    GetBalanceTableByVacationTypeForAccompaniment();
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Sports)') {
                    //console.log('this inside', result.data.VacationConsumedBalance);
                    // console.log('this inside', result.data);
                    if (result.data.SportsSeason != null) {
                        $("#txtSportsVacationBalance").val(result.data.VacationBalance);
                        $("#txtSportsVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                        $("#txtSportsVacationConsumedBalance").val(result.data.VacationConsumedBalance);
                        $("#txtSportsSeasonStartDate").val(RenderDate(result.data.SportsSeason.SportsSeasonStartDate));
                        $("#txtSportsSeasonEndDate").val(RenderDate(result.data.SportsSeason.SportsSeasonEndDate));
                    }
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Born)') {
                    $("#txtBornVacationBalance").val(result.data.VacationBalance);
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Emergency)') {
                    $("#txtEmergencyVacationBalance").val(result.data.VacationBalance);
                    $("#txtEmergencyVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                    $("#txtEmergencyVacationConsumedBalance").val(result.data.VacationConsumedBalance);
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Dead)') {
                    $("#txtDeadVacationBalance").val(result.data.VacationBalance);
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.MotherHood)') {
                    $("#txtMotherHoodVacationBalance").val(result.data.VacationBalance);
                    $("#txtMotherHoodVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                    $("#txtMotherHoodVacationConsumedBalance").val(result.data.VacationConsumedBalance);
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Birth)') {

                    $("#txtBirthVacationBalance").val(result.data.VacationBalance);
                    //$("#txtBirthVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                    //$("#txtBirthVacationConsumedBalance").val(result.data.VacationConsumedBalance);
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Studies)') {

                    //$("#txtStudiesVacationBalance").val(result.data.VacationBalance);
                    //$("#txtStudiesVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                    //$("#txtStudiesVacationConsumedBalance").val(result.data.VacationConsumedBalance);
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Compensatory)') {
                    $("#txtCompensatoryVacationBalance").val(result.data.VacationBalance);
                    $("#txtCompensatoryVacationRemainingBalance").val(result.data.VacationRemainingBalance);
                    $("#txtCompensatoryVacationConsumedBalance").val(result.data.VacationConsumedBalance);
                    $("#txtCompensatoryVacationExpiryDateText").val(RenderDate(result.data.VacationExpiredDate) == '1355-12-NaN' ? '' : RenderDate(result.data.VacationExpiredDate));
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Normal)') {

                    $("#txtNormalDeservedOldBalance").val(result.data.DeservedOldBalance);
                    $("#txtNormalRemainingOldBalance").val(result.data.RemainingOldBalance);
                    $("#txtNormalConsumedOldBalance").val(result.data.ConsumedOldBalance);

                    $("#txtNormalDeservedCurrentBalance").val(result.data.DeservedCurrentBalance);
                    $("#txtNormalConsumedCurrentBalance").val(result.data.ConsumedCurrentBalance);
                    $("#txtNormalExpiredCurrentBalance").val(result.data.ExpiredCurrentBalance);
                    $("#txtNormalRemainingCurrentBalance").val(result.data.RemainingCurrentBalance);

                    $("#txtNormalVacationInAdvanceBalance").val(result.data.InAdvanceBalance);

                    $("#txtNormalVacationTotalVacationBalance").val(result.data.TotalAvailableVacationBalance);

                    $("#txtNormalVacationTotalConsumedSeparatedDays").val(result.data.TotalConsumedSeparatedDays);

                    $("#txtNormalTotalDeservedBalance").val(result.data.TotalDeservedBalance);
                    $("#txtNormalTotalConsumedBalance").val(result.data.TotalConsumedBalance);
                    $("#txtNormalTotalRemainingBalance").val(result.data.TotalRemainingBalance);

                    if (result.data.CurrentBalance)
                        $('#btnCheckExpiry').removeAttr('disabled');
                }
                else if (VacationTypeID == '@Convert.ToInt16(@VacationsTypesEnum.Marriage)') {
                    $("#txtMarriageVacationBalance").val(result.data.VacationBalance);
                    $("#txtMarriageVacationConsumedBalance").val(result.data.VacationConsumedBalance);
                    $("#txtMarriageVacationRemainingBalance").val(result.data.VacationRemainingBalance);
